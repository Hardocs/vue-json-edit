{
  "name": "@hardocs-project/vue-json-edit",
  "version": "0.9.4",
  "description": "Hardocs Project highly modified visual JSON editor built as an vue component",
  "author": "Narration SD<narrationsd@gmail.com>, Jinkin1995 <jinkin1995@gmail.com>",
  "keywords": [
    "vue",
    "json"
  ],
  "main": "./src/index.js",
  "module": "./dist/vue-json-edit.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hardocs/vue-json-edit.git"
  },
  "homepage": "https://github.com/Hardocs/vue-json-edit",
  "bugs": {
    "url": "https://github.com/Hardocs/vue-json-edit/issues"
  },
  "license": "Apache 2",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.config.js",
    "example": "rm -rf ./example/dist/*.js && rm -rf ./example/dist/*.html && webpack --progress --config webpack.config.js",
    "build": "webpack --progress --config webpack.build.js",
    "drypub": "npm publish --access public --dry-run",
    "vpub": "npm publish --registry http://localhost:4873",
    "ppub": "npm publish --access public",
    "apub": "npm publish --access public --tag alpha",
    "bpub": "npm publish --access public --tag beta",
    "rpub": "npm publish --access public --tag rc",
    "npub": "npm publish --access public --tag next"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^7.1.0",
    "resolve-url-loader": "^3.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "highlight.js": "^10.4.1",
    "lodash.clonedeep": "^4.5.0",
    "vue": "^2.6.8",
    "vuedraggable": "^2.23.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
